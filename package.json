{
  "name": "cloudhost-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development DEBUG=cloudhost:* nodemon -L --exec babel-node -- ./src/app.js",
    "start": "node dist/app.js",
    "build": "rm -rf dist && babel src -s -D -d dist",
    "docker:start": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "docker:rebuild:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml build",
    "docker:rebuild:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml build"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cookie-parser": "^1.4.3",
    "debug": "^4.1.1",
    "express": "^4.16.4",
    "morgan": "^1.9.1",
    "tmp-promise": "^1.0.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "cross-env": "^5.2.0",
    "nodemon": "^1.18.9"
  }
}
